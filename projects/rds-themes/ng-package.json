{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/rds-themes",
  "deleteDestPath": false,
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": [
      "./styles/*.*",
      "./styles/fonts/*.*"
    ]
  },
  "assets": [
    "./styles/*.*",
    "./styles/fonts/*.*"
  ],
  "allowedNonPeerDependencies": [
    "bootstrap",
    "tslib"
  ]
}